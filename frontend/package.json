{
	"name": "calculator2",
	"version": "1.0.0",
	"description": "Calculator Application",
	"main": "main.js",
	"scripts": {
		"prestart": "yarn run dev-build",
		"start": "electron ./dist",
		"start-dev": "set NODE_ENV=local&& yarn run dev-server",
		"dev-server": "webpack-dev-server --config webpack/webpack.dev.js --watch --hot --inline --progress --port 8080 --open --colors",
		"dev-build": "rimraf dist&&set NODE_ENV=dev&&webpack --config webpack/webpack.dev.build.js",
		"build": "rimraf calculator-win32-x64 && electron-packager ./dist calculator --platform=win32",
		"build-app": "yarn run dev-build && yarn run build",
		"postinstall": "typings i"
	},
	"author": "Natcharin Sukcharoensub",
	"license": "MIT",
	"devDependencies": {
		"copy-webpack-plugin": "^4.3.1",
		"css-loader": "^0.28.7",
		"electron": "^1.7.11",
		"electron-packager": "^10.1.1",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.6",
		"html-webpack-plugin": "^2.30.1",
		"json-loader": "^0.5.7",
		"ng-annotate-webpack-plugin": "^0.2.1-pre",
		"node-sass": "^4.7.2",
		"on-build-webpack": "^0.1.0",
		"raw-loader": "^0.5.1",
		"rimraf": "^2.6.2",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.19.1",
		"ts-loader": "^3.2.0",
		"tslint": "^5.8.0",
		"tslint-loader": "^3.5.3",
		"typescript": "^2.6.2",
		"typings": "^2.1.1",
		"webpack": "^3.10.0",
		"webpack-dev-server": "^2.9.7",
		"webpack-merge": "^4.1.1"
	},
	"dependencies": {
		"@uirouter/angularjs": "^1.0.12",
		"angular": "1.5.8",
		"body-parser": "^1.18.2",
		"bulma": "^0.6.1",
		"font-awesome": "^4.7.0",
		"jquery": "^3.2.1"
	}
}
